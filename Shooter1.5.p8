 	e.y+=0.05
  if e.y >= 8 then
   e.y=8
  end
  --touche puteu
  for b in all(bullets) do
   if collision(e,b,4) then
    del(bullets,b)
    sfx(1)
    create_explosion(b.x+4,b.y+2)
    e.life-=1
    if e.life==0 then
     sfx(2)
     del(puteu,e)
     startp = 300
     score+=100
