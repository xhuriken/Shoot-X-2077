function create_explosion(a,b)
 sfx(1)
 add(explosions,
 {x=a,y=b,timer=0})
 
end

function update_explosions()
 for e in all(explosions) do
  e.timer+=1
  if e.timer == 13 then
   del(explosions,e)
  end
 end
end

function draw_explosions()
 circ(x,y,rayon,couleur)
 
 for e in all(explosions) do
  circ(e.x,
       e.y,
       e.timer/3,
       8+e.timer%3)
 end
end

function create_explosionj(a,b)
 sfx(1)
 add(explosionsj,
 {x=a,y=b,timer=0})
 
end

function update_explosionsj()
 for ex in all(explosionsj) do
  ex.timer+=1
  if ex.timer == 30 then
   del(explosionsj,ex)
  end
