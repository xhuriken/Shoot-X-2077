	konass={}
	puteu={}
 rakai={}
 --balles
	bullets={}
	bulletsr={}
	bulletsk={}
	bulletsp={}
	--explosions
	explosions={}
	explosionsj={}
	--divers
	create_stars()
	score=0
	one_life=50
	--spawn_var
	starte = 50
 startr = 1000
 startk = 3000
 startp = 5000
end

function _update60()
	for e in all(enemies) do
 	e.t += 1/60
	end
	if state== 0 then
 	starte -= 1
 	startr -= 1
 	startk -= 1
  startp -= 1
 end
 if state==0 then
  shootr()
  shootk()
	 shootp()
	end
 if (state==0) update_game()
 if (state==1) update_gameover()
 if (state==2) update_gamestart()
 if (state==3) update_gamehelp()
end

function _draw()
 if (state==0) draw_game()
 if (state==1) draw_gameover()
 if (state==2) draw_gamestart()
 if (state==3) draw_gamehelp()
end

function update_game()
 --update_divers
 update_explosions()
	update_explosionsj()
 update_stars()
 --update_entitees
 update_player()
 update_enemies()
	update_rakai()
	update_konass()
	update_puteu()
	--update_balles
	update_bullets()
	update_bulletsr()
	update_bulletsk()
 update_bulletsp()
	--spawn
	if starte<=0 then
	 spawn_enemies(6+flr(rnd(5)))
	 starte=200
	end	
	if startr <= 0 then
 	if #rakai==0 then
 	 spawn_rakai(2)
 	 for b in all(bulletsr) do
 	  del(bulletsr,b)
 	  create_explosionj(b.x,b.y)
 	 end
	 end
	end
	if startk <= 0 then
	 if #konass==0 then
	  spawn_konass(1)
	  for b in all(bulletsk) do
	   del(bulletsk,b)
	   create_explosionj(b.x,b.y)
	  end
	 end
	end
	if startp <= 0 then
	if #puteu==0 then
 	 spawn_puteu(2)
 	 for b in all(bulletsp) do
 	  del(bulletsp,b)
 	  create_explosionj(b.x,b.y)
 	 end
	 end
	end
end

