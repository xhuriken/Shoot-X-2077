function update_bulletsk()
	for b in all(bulletsk) do
		b.y+=b.speed
		if b.y>=128 then
			del(bulletsk,b)
		end
	end
end

--ballesjoueur
compteur = 1
function shoot()
	compteur -=1
	if compteur==0 then
	new_bullet={
		x=p.x,
		y=p.y,
		speed=4
	}
	compteur = 10	
	add(bullets,new_bullet)
	sfx(0)
	end
end

function update_bullets()
	for b in all(bullets) do
		b.y-=b.speed
		if b.y<-4 then
			del(bullets,b)
		end
	end
end
