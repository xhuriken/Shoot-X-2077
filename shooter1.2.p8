ck = 1
function shootkonass()
ck-=1
 if ck==0 then
 	new_bulletk={
			x=konass[1].x,
			y=konass[1].y,
			speed=2
		}
		ck=30
		add(bulletsk,new_bulletk)
		sfx(0)
	end
