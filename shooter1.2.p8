--enemies
function spawn_konass(x)
 for i=1,x do
  new_enemy={ 
   x=rnd(128),
   y=-30,
   life=3
  }
 	add(konass,new_enemy)
 end
end

function update_konass()
 for e in all(konass) do
  
 	if e.x>p.x+1 then
 	e.x-=0.3
 	end
 	if e.x<p.x+1 then
 	e.x+=0.3
 	end
 	if e.y>p.y+1 then
 	e.y-=0.3
 	end
 	if e.y<p.y+1 then
 	e.y+=0.5
 	end
  if e.y > 128 then
   del(konass,e)
  end
  --touche
  for b in all(bullets) do
   if collision(e,b,4) then
    del(bullets,b)
    sfx(1)
    create_explosion(b.x+4,b.y+2)
    e.life-=1
    if e.life==0 then
     sfx(2)
     del(konass,e)
     score+=50
     p.s+=0.01
    end
   end
  end
 end
end

function spawn_enemies(x)
 ecart=(128-8*x)/(x+1)*2
 for i=1,x do
  new_enemy={ 
   x=ecart*i+8*(i-1)-15,
   y=-30,
   life=2,
   t=0
  }
  
 	add(enemies,new_enemy)
 end
end

function update_enemies()
 for e in all(enemies) do
  e.y+=0.3
  e.x+=0.5*sin(e.t/3)
  
  if 
  e.y > 128 then
   del(enemies,e)
  end
  --touche
  for b in all(bullets) do
   if collision(e,b,4) then
      del(bullets,b)
    sfx(1)
    create_explosion(b.x+4,b.y+2)
    e.life-=1
    if e.life==0 then
     sfx(2)
     del(enemies,e)
     score+=10
     p.s+=0.05
    end
   end
  end
 end
end
