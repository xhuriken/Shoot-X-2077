function _init()
	lifej=3
	state=2
	p={x=60,y=110,s=1}
	bulletsk={}
	bullets={}
	enemies={}
	konass={}
	explosions={}
	explosionsj={}
	create_stars()
	score=0

end

function _update60()
	for e in all(enemies) do
 	e.t += 1/60
	end
 if (state==0) update_game()
 if (state==1) update_gameover()
 if (state==2) update_gamestart()
 if (state==3) update_gamehelp()
end

function _draw()
 if (state==0) draw_game()
 if (state==1) draw_gameover()
 if (state==2) draw_gamestart()
 if (state==3) draw_gamehelp()
end

decompte=0
function update_game()
 update_player()
	update_bullets()
 update_bulletsk()
	update_stars()
	if decompte==0 then
	 spawn_enemies(6+flr(rnd(5)))
	 decompte=200
	end
		decompte-=1
	if #konass==0 then
	 spawn_konass(1)
	end
	update_enemies()
	update_konass()
	update_explosions()
	update_explosionsj()
		
end


