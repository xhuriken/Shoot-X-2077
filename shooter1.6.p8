function spawn_enemies(x)
 	ecart=(128-8*x)/(x+1)*2
 	for i=1,x do
 	 	new_enemy={ 
 	  		x=ecart*i+8*(i-1)-15,
 	  		y=-30,
 	  		life=2,
 	  		t=0
 	 	}
 		add(enemies,new_enemy)
 	end
end

function update_enemies()
 	for e in all(enemies) do
 	 	e.y+=0.3
 	 	e.x+=0.5*sin(e.t/3)	
 	 	if e.y > 128 then
 	  		del(enemies,e)
 	 	end
 	 	--touche enemie
 	 	for b in all(bullets) do
 	  		if collision(e,b) then
 	  		   del(bullets,b)
 	  		 	sfx(1)
 	  		 	create_explosion(b.x+4,b.y+2)
 	  		 	e.life-=1
 	  		 	if e.life==0 then
 	  		  		sfx(2)
 	  		  		del(enemies,e)
 	  		  		score+=10
 	  		 	end
 	  		end
 	 	end
 	end
end
