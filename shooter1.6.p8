function create_stars()
	stars={}
	for i=1,10 do
	 	new_star={
	 		x=rnd(128),
	 		y=rnd(128),
	 		col=6,
	 		speed=3+rnd(3)
	 	}
	 	add(stars,new_star)
	end
	for i=1,14 do
	 	new_star={
	 		x=rnd(128),
	 		y=rnd(128),
	 		col=1,
	 		speed=2+rnd(2)
	 	}
	 	add(stars,new_star)
	end
end

function update_stars()
 	for s in all(stars) do
 		s.y+=s.speed
 		if s.y > 128 then
 		 	s.y=0
 		 	s.x=rnd(128)
 		end
 	end
end

