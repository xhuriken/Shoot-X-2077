function spawn_puteu(x)
 	for i=1,x do
  		new_enemy={ 
  				x=rnd(128),
   			y=-8,
   			life=10
  		}
 		add(puteu,new_enemy)
 	end
end

function update_puteu()
	if startp<=0 then
		puteuisready = 1
	end
	for e in all(puteu) do
		e.y+=0.05
		if e.y >= 20 then
			e.y=20
		end
		--touche puteu
		for b in all(bullets) do
			if collision(e,b) then
				del(bullets,b)
				sfx(1)
				create_explosion(b.x+4,b.y+2)
				e.life-=1
				if e.life==0 then
					sfx(2)
					del(puteu,e)
					for b in all(bulletsp) do
						del(bulletsp,b)
						create_explosionj(b.x,b.y)
					end
				end
			startp = 300
			score+=100
			end
		end
		--touche ballep
		for b in all(bullets) do
			for bp in all(bulletsp) do
			
				if collision(bp,b) then
					del(bullets,b)
					sfx(1)
					create_explosion(b.x+4,b.y+2)
					bp.life-=1
					if bp.life==0 then
						sfx(2)
						del(bulletsp,bp)
					end
				end
			end
		end
	end
end
