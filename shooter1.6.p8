function gameoverrequest()
	if lifej==0 then
 		state=1
	 	lastbtn_x = 1
	end
end

function clearmobs()
	create_explosionlife(p.x+4,p.y+2)
	for b in all(bullets) do
		del(bullets,b)
	end
	for b in all(bulletsk) do
		del(bulletsk,b)
	end
	for b in all(bulletsp) do
		del(bulletsp,b)
	end
	for b in all(bulletsr) do
		del(bulletsr,b)
	end
	for e in all(enemies) do
		del(enemies,e)
	end
	if rakaiisready == 1 then
		for e in all(rakai) do
			del(rakai,e)
		end
	end
	if puteuisready == 1 then
		for e in all(puteu) do
			del(puteu,e)
		end
	end
	if konassisready == 1 then
		for e in all(konass) do
			del(konass,e)
		end
	end
end
