cp = 1
function shootp()
	cp-=1
	if cp==0 then
		for pute in all(puteu) do
			new_bulletp={
				x=pute.x,
				y=pute.y,
				speed=0.3,
				life=2,
				dx=(p.x-pute.x)/sqrt((p.x-pute.x)*(p.x-pute.x)+(p.y-pute.y)*(p.y-pute.y)),
				dy=(p.y-pute.y)/sqrt((p.x-pute.x)*(p.x-pute.x)+(p.y-pute.y)*(p.y-pute.y))
			}
			add(bulletsp,new_bulletp)
			sfx(0)
		end	
		cp=250
	end
end

function update_bulletsp()
	for b in all(bulletsp) do
		b.y+=b.speed*b.dy
		b.x+=b.speed*b.dx
  		b.dy=(p.y-b.y)/sqrt((p.y-b.y)*(p.y-b.y)+(p.x-b.x)*(p.x-b.x))
  		b.dx=(p.x-b.x)/sqrt((p.y-b.y)*(p.y-b.y)+(p.x-b.x)*(p.x-b.x))
		if b.y>=128 then
			del(bulletsp,b)
		end
	end
end
