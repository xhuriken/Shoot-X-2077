function _init()
	lifej=3
	state=2
	p={x=60,y=110,s=1}
	--enemies
	enemies={}
	konass={}
	puteu={}
	rakai={}
	--balles
	bullets={}
	bulletsr={}
	bulletsk={}
	bulletsp={}
	--explosions
	explosions={}
	explosionsj={}
	--divers
	create_stars()
	score=0
	one_life=50
	tempscredits=50
	--spawn_var
	starte = 100
 	startr = 1000
	rakaiisready = 0
 	startk = 1500
	konassisready = 0
 	startp = 2000
	puteuisready = 0
end

function _update60()
	for e in all(enemies) do
 		e.t += 1/60
	end
	if state== 0 then
 		starte -= 1
 		startr -= 1
 		startk -= 1
  		startp -= 1
	end
	if state==0 then
  		shootr()
  		shootk()
		shootp()
	end
	if (state==0) update_game()
	if (state==1) update_gameover()
	if (state==2) update_gamestart()
	if (state==3) update_gamehelp()
	if (state==4) update_gamecredits()
end

function _draw()
 	if (state==0) draw_game()
 	if (state==1) draw_gameover()
 	if (state==2) draw_gamestart()
 	if (state==3) draw_gamehelp()
 	if (state==4) draw_gamecredits()
end

function update_game()
 	--update_divers
 	update_explosions()
	update_explosionsj()
 	update_stars()
 	--update_entitees
 	update_player()
 	update_enemies()
	update_rakai()
	update_konass()
	update_puteu()
	--update_balles
	update_bullets()
	update_bulletsr()
	update_bulletsk()
 	update_bulletsp()
	--spawn
	if starte<=0 then
		spawn_enemies(6+flr(rnd(5)))
		starte=200
	end	
	if startr <= 0 then
 		if #rakai==0 then
 			spawn_rakai(2)
 				for b in all(bulletsr) do
 					del(bulletsr,b)
 					create_explosionj(b.x,b.y)
 				end
		end
	end
	if startk <= 0 then
	 	if #konass==0 then
	  		spawn_konass(1)
	  		for b in all(bulletsk) do
	   			del(bulletsk,b)
	   			create_explosionj(b.x,b.y)
	  		end
	 	end
	end
	if startp <= 0 then
		if #puteu==0 then
 	 		spawn_puteu(2)
 	 		for b in all(bulletsp) do
 	 			del(bulletsp,b)
 	  			create_explosionj(b.x,b.y)
 	 		end
	 	end
	end
end

function draw_game()
 	cls()
 	--etoiles
 	for s in all(stars) do
  		pset(s.x,s.y,s.col)
 	end
	--balles
 	for b in all(bullets) do
  		spr(2,b.x,b.y)
 	end
 	for b in all(bulletsk) do
  		spr(16,b.x,b.y)
 	end
 	for b in all(bulletsr) do
  		spr(5,b.x,b.y)
 	end
 	for b in all(bulletsp) do
  		spr(14,b.x,b.y)
 	end
 	--perso
  	spr(1,p.x,p.y)
 	--enemies
 	for e in all(enemies) do
  		spr(3,e.x,e.y)
 	end
 	for e in all(konass) do
  		spr(15,e.x,e.y)
 	end
 	for e in all(rakai) do
  		spr(4,e.x,e.y)
 	end
 	for e in all(puteu) do
  		spr(13,e.x,e.y)
 	end
 	draw_explosions()
 	draw_explosionsj()
 	print(score,4,10,10)
 	if lifej==3 then
  		print("♥♥♥",3,3,8)
 	end
  	if lifej==2 then
  		print("♥♥",3,3,8)
 	end
 	if lifej==1 then
  		if one_life>=40 then
   			sfx(7)
  		end
  		if one_life >=0 then
   			print("♥",3,3,8)
  		end
  		if one_life<=-50 then
   			one_life=50
  		end
  		one_life -=1
 	end
end
